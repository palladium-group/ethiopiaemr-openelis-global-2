{
  "$schema": "https://openelis-global.org/schemas/analyzer-defaults/1.0",
  "analyzer_name": "Sysmex XN Series (XN-L, XN-1000)",
  "manufacturer": "Sysmex",
  "category": "HEMATOLOGY",
  "protocol": { "name": "ASTM", "version": "E1381-02" },
  "identifier_pattern": "SYSMEX.*XN-L|XN-1000|XN.*Series",
  "transport": ["RS-232", "TCP/IP"],
  "transport_config": {
    "RS-232": { "default_baud_rate": 19200 },
    "TCP/IP": {}
  },
  "default_test_mappings": [
    {
      "analyzer_code": "WBC",
      "test_name_hint": "White Blood Cells",
      "loinc": "6690-2",
      "unit": "10^3/uL"
    },
    {
      "analyzer_code": "RBC",
      "test_name_hint": "Red Blood Cells",
      "loinc": "789-8",
      "unit": "10^6/uL"
    },
    {
      "analyzer_code": "HGB",
      "test_name_hint": "Hemoglobin",
      "loinc": "718-7",
      "unit": "g/dL"
    },
    {
      "analyzer_code": "HCT",
      "test_name_hint": "Hematocrit",
      "loinc": "4544-3",
      "unit": "%"
    },
    {
      "analyzer_code": "MCV",
      "test_name_hint": "Mean Corpuscular Volume",
      "loinc": "787-2",
      "unit": "fL"
    },
    {
      "analyzer_code": "MCH",
      "test_name_hint": "Mean Corpuscular Hemoglobin",
      "loinc": "785-6",
      "unit": "pg"
    },
    {
      "analyzer_code": "MCHC",
      "test_name_hint": "Mean Corpuscular Hemoglobin Concentration",
      "loinc": "786-4",
      "unit": "g/dL"
    },
    {
      "analyzer_code": "PLT",
      "test_name_hint": "Platelet Count",
      "loinc": "777-3",
      "unit": "10^3/uL"
    },
    {
      "analyzer_code": "NEUT",
      "test_name_hint": "Neutrophils",
      "loinc": "751-8",
      "unit": "%"
    },
    {
      "analyzer_code": "LYMPH",
      "test_name_hint": "Lymphocytes",
      "loinc": "736-9",
      "unit": "%"
    },
    {
      "analyzer_code": "MONO",
      "test_name_hint": "Monocytes",
      "loinc": "742-7",
      "unit": "%"
    },
    {
      "analyzer_code": "EOS",
      "test_name_hint": "Eosinophils",
      "loinc": "711-2",
      "unit": "%"
    },
    {
      "analyzer_code": "BASO",
      "test_name_hint": "Basophils",
      "loinc": "704-7",
      "unit": "%"
    }
  ],
  "notes": "RESEARCH CORRECTION: Only ASTM over TCP/IP confirmed in vendor specifications. Original HL7 claim could not be verified. TCP/IP mode uses ASTM E1394-97 presentation layer + E1381-02 data link layer. RS-232 mode also available with baud rate 19200."
}
